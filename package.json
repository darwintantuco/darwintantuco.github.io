{
  "name": "dcrtantuco.github.io",
  "private": true,
  "description": "My personal website",
  "version": "0.1.0",
  "license": "MIT",
  "prettier": "@dcrtantuco/prettier-config",
  "eslintConfig": {
    "extends": [
      "@dcrtantuco/eslint-config"
    ]
  },
  "stylelint": {
    "extends": [
      "@dcrtantuco/stylelint-config"
    ]
  },
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "deploy": "gatsby build && gh-pages -d public -b master",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing \"",
    "prettier:check": "prettier --list-different \"src/**/*.{css,scss,js,jsx,json,md}\"",
    "prettier:fix": "prettier --write \"src/**/*.{css,scss,js,jsx,json,md}\"",
    "lint:js": "eslint 'src/**/*.js'",
    "lint:css": "stylelint 'src/**/*.{css,scss}'"
  },
  "dependencies": {
    "gatsby": "^2.13.73",
    "gatsby-plugin-sass": "^2.1.13",
    "node-sass": "^4.12.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "sanitize.css": "^11.0.0"
  },
  "devDependencies": {
    "@dcrtantuco/eslint-config": "^1.0.2",
    "@dcrtantuco/prettier-config": "^1.0.1",
    "@dcrtantuco/stylelint-config": "^1.0.1",
    "eslint": "^6.3.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "gh-pages": "^2.1.1",
    "prettier": "^1.18.2",
    "stylelint": "^10.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
